<div class="panel panel-default">
  <!-- Default panel contents -->  
  <div class="panel-heading">        
        <#=config.title#>
  </div>
  <div class="panel-body">
    <div class="form-horizontal form" role="form">
        <div class="row">
            <div class="col-md-4">
                <div class="form-group">
                    <label  class="col-md-3 control-label">select</label>
                    <div class="col-md-9 selectContainer">
                        
                    </div>
                </div>
            </div> 
            <div class="col-md-6">
                <div class="form-group">
                    <label  class="col-md-4 control-label">checkbox</label>
                    <div class="col-md-3 checkboxContainer">
                        
                    </div>
                    <button class="btn btn-default btn-on col-md-2">on</button>  
                    <button class="btn btn-default btn-off col-md-2">off</button>  
                </div>
            </div>                   
        </div>  
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label  class="col-md-1 control-label">radio</label>
                    <div class="col-md-1 radioContainer">
                        
                    </div>
                    <div class="col-md-10">
                        <div class="row">
                            <div class="form-group">
                                <button class="btn btn-default btn-a col-md-1">A</button>  
                                <button class="btn btn-default btn-b col-md-1">B</button>                                  
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">                             
                                <button class="btn btn-default btn-radio-enable col-md-1">enable</button>  
                                <button class="btn btn-default btn-radio-disable col-md-1">disable</button>  
                            </div>
                        </div>   
                        <div class="row">
                            <div class="form-group">                             
                                <button class="btn btn-default btn-radio-color col-md-1">blue</button>  
                                <button class="btn btn-default btn-radio-color col-md-1">red</button>  
                                <button class="btn btn-default btn-radio-color col-md-1">green</button>
                                <button class="btn btn-default btn-radio-color col-md-1">yellow</button>    
                                <button class="btn btn-default btn-radio-color col-md-1">orange</button>    
                            </div>
                        </div>                      
                    </div>

                </div>
            </div> 
        </div>  
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label  class="col-md-1 control-label">combobox</label>
                    <div class="col-md-3 comboboxContainer">                        
                    </div>
                    <div class="col-md-6 col-md-offset-1">
                        <div class="row">
                            <div class="form-group">
                                <button class="btn btn-default btn-combobox-a col-md-1">选项1</button>  
                                <button class="btn btn-default btn-combobox-b col-md-1">选项2</button>  
                                <button class="btn btn-default btn-combobox-c col-md-1">选项3</button>                                  
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">                             
                                <button class="btn btn-default btn-combobox-enable col-md-1">enable</button>  
                                <button class="btn btn-default btn-combobox-disable col-md-1">disable</button>  
                            </div>
                        </div>   
                        <div class="row">
                            <div class="form-group">                             
                                <button class="btn btn-default btn-combobox-c3 col-md-1">3选项</button>  
                                <button class="btn btn-default btn-combobox-c5 col-md-1">5选项</button>  
                            </div>
                        </div>                      
                    </div>
                </div>
            </div> 
        </div>       
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label  class="col-md-1 control-label">UEditor</label>
                    <div class="col-md-5 ueditorContainer">                        
                    </div>
                    <div class="col-md-5 col-md-offset-1">                        
                        <div class="row">
                            <div class="form-group">                             
                                <button class="btn btn-default btn-ue-enable col-md-2">enable</button>  
                                <button class="btn btn-default btn-ue-disable col-md-2">disable</button>  
                                <button class="btn btn-default btn-height-300 col-md-2">height=300px</button>  
                                <button class="btn btn-default btn-height-900 col-md-2">height=900px</button>  
                            </div>
                        </div>                                            
                    </div>
                </div>
            </div> 
        </div> 
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label  class="col-md-1 control-label">codeMirror</label>
                    <div class="col-md-5 codeMirror">                        
                    </div>
                    <div class="col-md-5 col-md-offset-1">                        
                        <div class="row">
                            <div class="form-group">                             
                                <button class="btn btn-default btn-code-enable col-md-2">enable</button>  
                                <button class="btn btn-default btn-code-disable col-md-2">disable</button>  
                                <button class="btn btn-default btn-code-getvalue col-md-2">get value</button>
                                <button class="btn btn-default btn-code-setvalue col-md-2">set value</button>
                                <!-- <button class="btn btn-default btn-height-300 col-md-2">height=300px</button>  
                                <button class="btn btn-default btn-height-900 col-md-2">height=900px</button>   -->
                            </div>
                        </div>                                            
                    </div>
                </div>
            </div> 
        </div>
    </div> 
  </div>
  <div class="panel-footer">
       <div class="row">
            <div class="col-md-12  navbar-form navbar-right">
                <div class="form-group">
                    <div class="col-md-12">
                        <button class="btn btn-default btn-save">保存</button>                       
                        <button class="btn btn-default btn-cancel">返回</button>
                    </div>                  
                </div>  
            </div>
        </div> 
  </div>
</div>